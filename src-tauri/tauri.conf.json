{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:3906",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Red Blood Cell MD",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/icon.ico",
        "icons/rbc.png"
      ],
      "identifier": "com.tauri.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Red Blood Cell MD",
        "width": 1000,
        "height": 750,
        "minWidth": 600,
        "minHeight": 450
      }
    ]
  }
}
